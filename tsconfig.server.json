  "compilerOptions": {
    "target": "ES2022",
    "lib": ["ES2022"],
    "rootDir": ".",
    "outDir": "./dist/server",
    "module": "CommonJS",
    "esModuleInterop": true,
    "moduleResolution": "node",
    "strict": false,
    "skipLibCheck": true,
    "types": ["node"]
