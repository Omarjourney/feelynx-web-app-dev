# Copy this file to .env and customize it for your local setup.
# Do NOT commit the .env file to version control.

# Database configuration
DB_HOST=example_db_host
DB_PORT=example_db_port
DB_USER=example_db_user
DB_PASSWORD=example_db_password
DB_NAME=example_db_name

# Prisma configuration
DATABASE_URL=example_database_url
OPTIMIZE_API_KEY=example_optimize_api_key

# Authentication
JWT_SECRET=example_jwt_secret

# Stripe configuration
STRIPE_SECRET_KEY=sk_test_example
STRIPE_WEBHOOK_SECRET=whsec_example

# LiveKit configuration
LIVEKIT_URL=your_livekit_url_here
LIVEKIT_API_KEY=your_livekit_api_key_here
LIVEKIT_API_SECRET=your_livekit_api_secret_here
VITE_LIVEKIT_WS_URL=your_livekit_ws_url_here

# Supabase configuration
VITE_SUPABASE_URL=https://your-project.supabase.co
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key

# If migrating from Next.js, these map to Vite equivalents:
# NEXT_PUBLIC_SUPABASE_URL -> VITE_SUPABASE_URL
# NEXT_PUBLIC_SUPABASE_PUBLISHABLE_DEFAULT_KEY -> VITE_SUPABASE_ANON_KEY

# Server-side Supabase (do not expose in client)
SUPABASE_URL=https://your-project.supabase.co
# Service Role key is sensitive. Store only in server/hosting env.
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key

# Media assets
VITE_UNSPLASH_RANDOM_BASE_URL=https://source.unsplash.com/random/

# Allows camera/mic access on non-HTTPS hosts for development
# VITE_MEDIA_HOST_WHITELIST=dev.example.com
